	"keywords": ["wordpress", "xmlrpc", "client", "remote", "api", "multisite"],
	"license": "MIT",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/yipeecaiey/wordpress-xmlrpc-client"
      }
    ],
	"require": {
		"php": ">=5.3.0",
		"hieu-le/wordpress-xmlrpc-client": "dev-master"
	},
	"autoload": {
